                              <Button fx:id="botaoInscritos" mnemonicParsing="false" onAction="#analisarTotalInscritos" text="TOTAL DE INSCRITOS" textAlignment="JUSTIFY" BorderPane.alignment="CENTER" />
                           </center>
                        </BorderPane>
                        <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
                           <center>
                              <Button fx:id="botaoMediaProvaObjetiva" mnemonicParsing="false" onAction="#analisarMediaProvaObjetiva" text="MÉDIA DAS PROVAS OBJETIVAS" textAlignment="JUSTIFY" BorderPane.alignment="CENTER" />
